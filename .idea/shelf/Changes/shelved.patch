+// Підключення бібліотек
+#include "stdio.h"
+#include "math.h"
+#include "locale.h"
+
+int main(){
+    setlocale(LC_CTYPE, "ukr"); // Підключення української мови
+    double r, s; // Оголошення змінних
+    const double PI = 3.141592653589793; // Оголошення числа pi
+    printf("Введіть радіус кола: "); // Введення даних
+    scanf("%lf", &r);
+    s = (PI * pow(r, 2)) / 2; // Обчислення півплощі
+    printf("\n Шукане число= %.3f\n", s); // Виведення півплощі
+}
